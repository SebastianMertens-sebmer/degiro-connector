# DEGIRO Trading API Environment Configuration Template
# Copy this file to .env and fill in your credentials

# API Security
TRADING_API_KEY=your_secure_32_char_token_here

# DEGIRO Credentials
DEGIRO_USERNAME=your_username
DEGIRO_PASSWORD=your_password
DEGIRO_TOTP_SECRET=your_2fa_secret
DEGIRO_INT_ACCOUNT=your_int_account

# Application Settings
API_PORT=7731
DEBUG=false
LOG_LEVEL=INFO
MAX_WORKERS=4

# VPS Deployment (optional)
VPS_HOST=your.vps.ip.address
VPS_USER=your_vps_user
VPS_PATH=/home/user/degiro-trading-api